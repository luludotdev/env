{
  "name": "@lolpants/env",
  "version": "0.6.2",
  "packageManager": "yarn@3.2.4",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./register": {
      "import": "./dist/register.js",
      "types": "./dist/register.d.ts"
    },
    "./register.js": {
      "import": "./dist/register.js",
      "types": "./dist/register.d.ts"
    }
  },
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit --skipLibCheck",
    "lint": "prettier --check . && eslint src --ext ts",
    "format": "prettier --write . && eslint src --ext ts --fix",
    "fmt": "yarn run format",
    "test": "yarn run check && yarn run lint",
    "prepack": "yarn run test && yarn run build"
  },
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lolPants/env.git"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@lolpants/eslint-config": "^0.2.3",
    "@types/node": "^18.11.8",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv-cra": "^3.0.2"
  }
}
