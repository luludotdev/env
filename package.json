{
  "name": "@lolpants/env",
  "version": "0.6.0",
  "packageManager": "yarn@3.2.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./register": {
      "import": "./dist/register.mjs",
      "require": "./dist/register.js",
      "types": "./dist/register.d.ts"
    },
    "./register.js": {
      "import": "./dist/register.mjs",
      "require": "./dist/register.js",
      "types": "./dist/register.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "yarn run lint",
    "build": "tsup",
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "fmt": "yarn run format",
    "prepack": "yarn run test && yarn run build"
  },
  "repository": "git@github.com:lolPants/env.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.1",
    "@types/node": "^18.7.18",
    "eslint": "^8.23.1",
    "eslint-config-neon": "^0.1.33",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "dotenv-cra": "^3.0.2"
  }
}
