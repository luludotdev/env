{
  "name": "@luludev/env",
  "version": "0.6.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./register": {
      "import": "./dist/register.js",
      "types": "./dist/register.d.ts"
    },
    "./register.js": {
      "import": "./dist/register.js",
      "types": "./dist/register.d.ts"
    }
  },
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit --skipLibCheck",
    "lint": "prettier --check . && eslint src --ext ts",
    "format": "prettier --write . && eslint src --ext ts --fix",
    "fmt": "npm run format",
    "test": "npm run check && npm run lint",
    "prepack": "npm run test && npm run build"
  },
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luludotdev/env.git"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@luludev/eslint-config": "^0.4.2",
    "@types/node": "^20.4.2",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv-cra": "^3.0.3"
  }
}
